{
  "name": "utils4u",
  "version": "2.12.0",
  "description": "Utilities for you",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanhao98/utils4u.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite/index.d.mts",
      "default": "./dist/vite/index.mjs"
    },
    "./vue-router": {
      "types": "./dist/vue-router/index.d.mts",
      "default": "./dist/vue-router/index.mjs"
    },
    "./rollup": {
      "types": "./dist/rollup/index.d.mts",
      "default": "./dist/rollup/index.mjs"
    },
    "./tdesign-mobile-vue": {
      "types": "./dist/tdesign-mobile-vue/index.d.ts",
      "default": "./dist/tdesign-mobile-vue/index.js"
    },
    "./vant": {
      "types": "./dist/vant/index.d.ts",
      "default": "./dist/vant/index.js"
    },
    "./vue-use": {
      "types": "./dist/vue-use/index.d.mts",
      "default": "./dist/vue-use/index.mjs"
    },
    "./browser": {
      "types": "./dist/browser/index.d.mts",
      "default": "./dist/browser/index.mjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "esno scripts/build.ts",
    "build:watch": "unbuild --watch",
    "build:stub": "unbuild --stub",
    "dev": "vitest dev",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "automd && eslint . --fix && prettier -w .",
    "prepack": "pnpm build",
    "play": "jiti playground",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.11",
    "@rsbuild/plugin-babel": "1.0.3",
    "@rsbuild/plugin-vue": "1.0.4",
    "@rsbuild/plugin-vue-jsx": "1.0.1",
    "@rslib/core": "^0.0.18",
    "@types/node": "^22.9.0",
    "@types/nprogress": "^0.2.3",
    "@vitest/coverage-v8": "^2.1.5",
    "@vue/test-utils": "^2.4.6",
    "automd": "^0.3.12",
    "changelogen": "^0.5.7",
    "dayjs": "^1.11.13",
    "eslint": "^9.14.0",
    "eslint-config-unjs": "^0.4.1",
    "esno": "^4.8.0",
    "fast-glob": "^3.3.2",
    "happy-dom": "^15.11.6",
    "jiti": "2.0.0-rc.1",
    "jsdom": "^25.0.1",
    "nprogress": "^0.2.0",
    "pinyin-pro": "^3.26.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tdesign-mobile-vue": "^1.5.0",
    "typescript": "^5.6.3",
    "unbuild": "rc",
    "vant": "^4.9.8",
    "vite": "^5.4.11",
    "vitest": "^2.1.5",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vue-router-mock": "^1.1.0"
  },
  "packageManager": "pnpm@9.13.0"
}
